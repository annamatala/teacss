Script teacss {
    // Core
    @append "teacss/core.js";
    
    // Color
    @append "teacss/color.js";
    
    // Image
    @append "teacss/image.js";
    
    // Build
    @append "teacss/build/build.js";
    
    // Canvas
    @append 'teacss/canvas/canvas.js';
    @append 'teacss/canvas/effects.js';
    @append 'teacss/canvas/shader.js';
    @append 'teacss/canvas/texture.js';
    
    @append {
        teacss.update();
    }
}