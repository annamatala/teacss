@ var a = 2;
@ var x = a * a;
@ var y = x + 1;
@ var z = x * 2 + y;

.variables {
  width: @{z + 1}cm; // 14cm
}

@ var b = a * 10;
@ var c = color('#888');

@ var fonts = '"Trebuchet MS", Verdana, sans-serif';
@ var f = fonts;

@ var quotes = '"~" "~"';
@ var q = quotes;

.variables {
  height: @{b + x + 0}px; // 24px
  color: @c;
  font-family: @f;
  quotes: @q;
}

.redefinition {
    @ var v = 4;
    @ var v = 2;
    @ var v = 3;
    three: @v;
}

.values {
    @ var a = "'Trebuchet'";
    @ var multi = "'A', B, C";
    font-family: @a, @a, @a;
    color: @c !important;
    url: url(@a);
    multi: something @multi, @a;
}

.variable-names {
    @ var v = "'hello'";
    @ var name = 'v';
    name: @{eval(name)};
}
.alpha {
    @ var v = 42;
    filter: alpha(opacity=@v);
}

a:nth-child(@a) {
    border: 1px;
}
