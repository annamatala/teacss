.parens {
  @ var v = 1;
  border: @{(v * 2)}px solid @{color('black')};
  margin: @{(v * 1)}px @{(v + 2)}px @{(4 * 4)} 3;
  width: @{(6 * 6)};
  padding: 2px @{(6 * 6)}px;
}

.more-parens {
  @ var v = (2 * 2);
  padding: @{(2 * v)} 4 4 @{(v * 1)}px;
  width: @{(v * v)*6};
  height: @{(7 * 7) + (8 * 8)};
  margin: @{4 * (5 + 5) / 2 - (v * 2)};
  //margin: (6 * 6)px;
}

.nested-parens {
  width: @{2 * (4 * (2 + (1 + 6))) - 1};
  height: @{((2+3)*(2+3) / (9-4)) + 1};
}

.mixed-units {
  margin: 2px 4em 1 5pc;
  padding: @{(2 + 4)}px 1em 2px 2;
}
